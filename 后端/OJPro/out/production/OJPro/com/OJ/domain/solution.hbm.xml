<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.OJ.domain.solution" table="solution">
        <!--建立类中的属性与表中的主键对应-->
        <id name="SSid" column="SSid" >
            <generator class="native"/>
        </id>
        <!--建立类中的普通的属性和表的字段的对应-->
        <property name="Pid" column="Pid" />
        <property name="Sid" column="Sid"/>
        <property name="TIid" column="TIid"/>
        <property name="score" column="score"/>
        <property name="submitDate" column="submitDate"/>
        <property name="memory" column="memory"/>
        <property name="time" column="time"/>
        <property name="codeLength" column="codeLength"/>
        <property name="ULid" column="ULid"/>
        <property name="JRid" column="JRid"/>
        <property name="error" column="error"/>
    </class>
</hibernate-mapping>